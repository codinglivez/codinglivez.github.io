<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Bucket List</name>
    <description>
        A simple todo application for noting your schedule.
    </description>
    <author email="programmer0608@gmail.com" href="https://codinglivez.github.io/ravi/">
        Ravivanshikumar Sangpal
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="img/logo.png" density="ldpi" />
        <icon src="img/logo.png" density="mdpi" />
        <icon src="img/logo.png" density="hdpi" />
        <icon src="img/logo.png" density="xhdpi" />
        <icon src="img/logo.png" density="xxhdpi" />
        <icon src="img/logo.png" density="xxxhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon src="img/logo.png" density="ldpi" />
        <icon src="img/logo.png" density="mdpi" />
        <icon src="img/logo.png" density="hdpi" />
        <icon src="img/logo.png" density="xhdpi" />
        <icon src="img/logo.png" density="xxhdpi" />
        <icon src="img/logo.png" density="xxxhdpi" />
    </platform>
</widget>
